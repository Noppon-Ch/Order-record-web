<%- include('partials/app-header') %>

    <div class="max-w-7xl mx-auto py-6 sm:px-6 lg:px-8">
        <div class="px-4 sm:px-0 mb-6">
            <div>
                <h1 class="text-3xl font-bold text-gray-900">ประวัติการสั่งซื้อ (Order History)</h1>
                <p class="mt-1 text-sm text-gray-600">ดูและจัดการรายการสั่งซื้อของลูกค้า
                </p>
            </div>
            <!-- Mobile: ปุ่มอยู่ใต้ข้อความ -->
            <div class="flex flex-col gap-2 mt-4 sm:hidden">
                <a href="/orders/first"
                    class="w-full justify-center inline-flex items-center px-4 py-3 border border-transparent rounded-md shadow-sm text-base font-medium text-white bg-blue-600 hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500">
                    <svg class="-ml-1 mr-2 h-5 w-5" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                        stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                            d="M9 13h6m-3-3v6m5 5H6a2 2 0 01-2-2V6a2 2 0 012-2h10a2 2 0 012 2v12a2 2 0 01-2 2z" />
                    </svg>
                    ใบสั่งซื้อรอบแรก
                </a>
                <a href="/orders/continue"
                    class="w-full justify-center inline-flex items-center px-4 py-3 border border-blue-600 rounded-md shadow-sm text-base font-medium text-blue-600 bg-white hover:bg-blue-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500">
                    <svg class="-ml-1 mr-2 h-5 w-5" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                        stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                            d="M9 13h6m-3-3v6m5 5H6a2 2 0 01-2-2V6a2 2 0 012-2h10a2 2 0 012 2v12a2 2 0 01-2 2z" />
                    </svg>
                    ใบสั่งซื้อต่อเนื่อง
                </a>
            </div>
            <!-- PC: ปุ่มอยู่ขวา -->
            <div class="hidden sm:flex gap-2 justify-end items-center mt-4">
                <a href="/orders/first"
                    class="inline-flex items-center px-4 py-2 border border-transparent rounded-md shadow-sm text-sm font-medium text-white bg-blue-600 hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500">
                    <svg class="-ml-1 mr-2 h-5 w-5" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                        stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                            d="M9 13h6m-3-3v6m5 5H6a2 2 0 01-2-2V6a2 2 0 012-2h10a2 2 0 012 2v12a2 2 0 01-2 2z" />
                    </svg>
                    ใบสั่งซื้อรอบแรก
                </a>
                <a href="/orders/continue"
                    class="inline-flex items-center px-4 py-2 border border-blue-600 rounded-md shadow-sm text-sm font-medium text-blue-600 bg-white hover:bg-blue-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500">
                    <svg class="-ml-1 mr-2 h-5 w-5" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                        stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                            d="M9 13h6m-3-3v6m5 5H6a2 2 0 01-2-2V6a2 2 0 012-2h10a2 2 0 012 2v12a2 2 0 01-2 2z" />
                    </svg>
                    ใบสั่งซื้อต่อเนื่อง
                </a>
            </div>
        </div>

        <!-- Search Box -->
        <div class="mb-6 px-4 sm:px-0">
            <div class="sm:flex sm:justify-end">
                <form action="/orders/history" method="GET"
                    class="relative rounded-md shadow-sm max-w-lg w-full sm:max-w-lg sm:w-full">
                    <input type="text" name="q" id="search"
                        class="focus:ring-blue-500 focus:border-blue-500 block w-full pl-4 pr-12 sm:text-sm border-gray-300 rounded-md py-3"
                        placeholder="ค้นหารายการ..." value="<%= query %>">
                    <div class="absolute inset-y-0 right-0 flex items-center">
                        <button type="submit"
                            class="inline-flex items-center px-4 py-2 border border-transparent text-sm font-medium rounded-r-md text-white bg-blue-600 hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500 h-full">
                            ค้นหา
                        </button>
                    </div>
                </form>
            </div>
        </div>

        <!-- Mobile Table View -->
        <div class="sm:hidden shadow border-b border-gray-200 rounded-lg overflow-hidden mb-4">
            <table class="min-w-full divide-y divide-gray-200">
                <thead class="bg-gray-50">
                    <tr>
                        <th scope="col"
                            class="px-4 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                            ข้อมูล
                        </th>
                        <th scope="col"
                            class="px-4 py-3 text-center text-xs font-medium text-gray-500 uppercase tracking-wider w-20">
                            จัดการ
                        </th>
                    </tr>
                </thead>
                <tbody class="bg-white divide-y divide-gray-200">
                    <% if (orders && orders.length> 0) { %>
                        <% orders.forEach((order, index)=> { %>
                            <% const positionClasses={ 'BM' : 'bg-blue-200 text-blue-700' , 'AG'
                                : 'bg-green-200 text-green-700' , 'SFAG' : 'bg-yellow-300 text-yellow-800' , 'SAG'
                                : 'bg-red-200 text-red-800' , 'ESAG' : 'bg-red-200 text-red-800' }; const
                                pos=(order.position || '' ).toUpperCase(); const badgeClass=positionClasses[pos]
                                || 'bg-gray-100 text-gray-800' ; %>
                                <tr data-order-index="<%= index %>"
                                    class="cursor-pointer hover:bg-gray-50 transition-colors">
                                    <td class="px-4 py-4 text-sm text-gray-900 align-top">
                                        <div class="flex flex-col gap-1">
                                            <div class="text-xs text-gray-500">
                                                <%= new Date(order.order_date).toLocaleDateString('th-TH') %>
                                            </div>
                                            <div class="flex flex-wrap items-center gap-2">
                                                <div class="text-sm font-bold text-gray-900">
                                                    <%= order.customers ? (order.customers.customer_fname_th + ' ' +
                                                        order.customers.customer_lname_th) : 'ไม่ระบุ' %>
                                                </div>
                                                <span
                                                    class="px-2 inline-flex text-xs leading-5 font-medium rounded-full <%= badgeClass %>">
                                                    <%= order.position || '-' %>
                                                </span>
                                            </div>
                                            <div class="text-sm text-gray-700 font-medium">
                                                <%= (order.order_total_amount / 100).toLocaleString('en-US', {
                                                    minimumFractionDigits: 0, maximumFractionDigits: 0 }) %> คะแนน
                                            </div>
                                        </div>
                                    </td>
                                    <td class="px-4 py-4 text-center whitespace-nowrap text-sm font-medium align-top"
                                        data-no-row-click="true">
                                        <div class="flex flex-row gap-2 justify-center">
                                            <% if (order.order_type==='c_order' ) { %>
                                                <a href="/orders/<%= order.order_id %>/continue-pdf" target="_blank"
                                                    class="text-indigo-600 hover:text-indigo-900 bg-indigo-50 px-2 py-1 rounded">PDF</a>
                                                <% } else { %>
                                                    <a href="/orders/<%= order.order_id %>/pdf" target="_blank"
                                                        class="text-indigo-600 hover:text-indigo-900 bg-indigo-50 px-2 py-1 rounded">PDF</a>
                                                    <% } %>
                                                        <button data-delete-order="<%= order.order_id %>"
                                                            class="text-red-600 hover:text-red-900 bg-red-50 px-2 py-1 rounded">ลบ</button>
                                        </div>
                                    </td>
                                </tr>
                                <% }) %>
                                    <% } else { %>
                                        <tr>
                                            <td colspan="2" class="px-6 py-10 text-center text-gray-500">
                                                ไม่พบรายการใบสั่งซื้อ
                                            </td>
                                        </tr>
                                        <% } %>
                </tbody>
            </table>
        </div>

        <!-- Data Table (Desktop) -->
        <div class="hidden sm:flex flex-col">
            <div class="-my-2 overflow-visible sm:-mx-6 lg:-mx-8">
                <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
                    <div class="shadow border-b border-gray-200 sm:rounded-lg">
                        <table class="min-w-full divide-y divide-gray-200">
                            <thead class="bg-gray-50">
                                <tr>
                                    <% if (['leader', 'co-leader' ].includes(userRole)) { %>
                                        <th scope="col"
                                            class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                            ผู้บันทึก
                                        </th>
                                        <% } %>
                                            <th scope="col"
                                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                วันที่
                                            </th>
                                            <th scope="col"
                                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                ประเภท
                                            </th>
                                            <th scope="col"
                                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                ลูกค้า
                                            </th>
                                            <th scope="col"
                                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                ตำแหน่ง
                                            </th>
                                            <th scope="col"
                                                class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                ผู้แนะนำ
                                            </th>
                                            <th scope="col"
                                                class="px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                คะแนน
                                            </th>
                                            <th scope="col"
                                                class="px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                ยอดสุทธิ
                                            </th>
                                            <th scope="col" class="relative px-6 py-3">
                                                <span class="sr-only">จัดการ</span>
                                            </th>
                                </tr>
                            </thead>
                            <tbody class="bg-white divide-y divide-gray-200">
                                <% if (orders && orders.length> 0) { %>
                                    <% orders.forEach((order, index)=> { %>
                                        <tr class="<%= index % 2 === 0 ? 'bg-white' : 'bg-gray-50' %> transform transition-transform duration-200 hover:scale-[1.02] hover:shadow-lg hover:z-50 relative cursor-pointer"
                                            data-order-index="<%= index %>">
                                            <% if (['leader', 'co-leader' ].includes(userRole)) { %>
                                                <td class="px-6 py-6 whitespace-nowrap text-sm text-gray-900">
                                                    <% if (order.recorder) { %>
                                                        <div class="flex items-center">
                                                            <div
                                                                class="h-8 w-8 rounded-full bg-blue-100 flex items-center justify-center text-blue-700 font-bold mr-2 text-xs">
                                                                <%= order.recorder.user_full_name ?
                                                                    order.recorder.user_full_name.charAt(0).toUpperCase()
                                                                    : '?' %>
                                                            </div>
                                                            <span>
                                                                <%= order.recorder.user_full_name || 'ไม่ระบุ' %>
                                                            </span>
                                                        </div>
                                                        <% } else { %>
                                                            <span class="text-gray-400">-</span>
                                                            <% } %>
                                                </td>
                                                <% } %>
                                                    <td class="px-6 py-6 whitespace-nowrap text-sm text-gray-900">
                                                        <%= new Date(order.order_date).toLocaleDateString('th-TH') %>
                                                    </td>
                                                    <td class="px-6 py-6 whitespace-nowrap text-sm text-gray-900">
                                                        <% if (order.order_type==='f_order' ) { %>
                                                            <span
                                                                class="px-2 inline-flex text-xs leading-5 font-medium rounded-full bg-green-100 text-green-800">
                                                                รอบแรก
                                                            </span>
                                                            <% } else if (order.order_type==='c_order' ) { %>
                                                                <span
                                                                    class="px-2 inline-flex text-xs leading-5 font-medium rounded-full bg-blue-100 text-blue-800">
                                                                    ต่อเนื่อง
                                                                </span>
                                                                <% } else { %>
                                                                    <span class="text-gray-500">-</span>
                                                                    <% } %>
                                                    </td>
                                                    <td class="px-6 py-6 whitespace-nowrap text-sm text-gray-900">
                                                        <% if (order.customers) { %>
                                                            <%= order.customers.customer_fname_th %>
                                                                <%= order.customers.customer_lname_th %>
                                                                    <% } else { %>
                                                                        <span class="text-gray-400">ไม่ระบุ</span>
                                                                        <% } %>
                                                    </td>
                                                    <td class="px-6 py-6 whitespace-nowrap text-sm text-gray-500">
                                                        <% const positionClasses={ 'BM' : 'bg-blue-200 text-blue-700'
                                                            , 'AG' : 'bg-green-200 text-green-700' , 'SFAG'
                                                            : 'bg-yellow-300 text-yellow-800' , 'SAG'
                                                            : 'bg-red-200 text-red-800' , 'ESAG'
                                                            : 'bg-red-200 text-red-800' }; const pos=(order.position
                                                            || '' ).toUpperCase(); const badgeClass=positionClasses[pos]
                                                            || 'bg-gray-100 text-gray-800' ; %>
                                                            <span
                                                                class="px-2 inline-flex text-xs leading-5 font-medium rounded-full <%= badgeClass %>">
                                                                <%= order.position || '-' %>
                                                            </span>
                                                    </td>
                                                    <td class="px-6 py-6 whitespace-nowrap text-sm text-gray-500">
                                                        <% if (order.recommender) { %>
                                                            <%= order.recommender.customer_fname_th %>
                                                                <%= order.recommender.customer_lname_th %>
                                                                    <% } else { %>
                                                                        -
                                                                        <% } %>
                                                    </td>
                                                    <td
                                                        class="px-6 py-6 whitespace-nowrap text-sm text-gray-900 text-right">
                                                        <%= (order.order_total_amount / 100).toLocaleString('en-US', {
                                                            minimumFractionDigits: 2 }) %>
                                                    </td>
                                                    <td
                                                        class="px-6 py-6 whitespace-nowrap text-sm font-medium text-gray-900 text-right">
                                                        <%= (order.order_final_price / 100).toLocaleString('en-US', {
                                                            minimumFractionDigits: 2 }) %>
                                                    </td>
                                                    <td class="px-6 py-6 whitespace-nowrap text-right text-sm font-medium space-x-2"
                                                        data-no-row-click="true">
                                                        <% if (order.order_type==='c_order' ) { %>
                                                            <a href="/orders/<%= order.order_id %>/continue-pdf"
                                                                target="_blank"
                                                                class="text-indigo-600 hover:text-indigo-900 z-20 relative">PDF</a>
                                                            <% } else { %>
                                                                <a href="/orders/<%= order.order_id %>/pdf"
                                                                    target="_blank"
                                                                    class="text-indigo-600 hover:text-indigo-900 z-20 relative">PDF</a>
                                                                <% } %>
                                                                    <button data-delete-order="<%= order.order_id %>"
                                                                        class="text-red-600 hover:text-red-900 z-20 relative">ลบ</button>
                                                    </td>
                                        </tr>
                                        <% }) %>
                                            <% } else { %>
                                                <tr>
                                                    <td colspan="<%= ['leader', 'co-leader'].includes(userRole) ? 9 : 8 %>"
                                                        class="px-6 py-10 text-center text-gray-500">
                                                        ไม่พบรายการใบสั่งซื้อ
                                                    </td>
                                                </tr>
                                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <!-- Order Details Modal -->
        <div id="orderModal" class="hidden relative z-50" aria-labelledby="modal-title" role="dialog" aria-modal="true">
            <div id="modalOverlay" class="fixed inset-0 bg-gray-500 bg-opacity-75 transition-opacity"
                aria-hidden="true"></div>
            <div class="fixed inset-0 z-10 w-screen overflow-y-auto">
                <div class="flex min-h-full items-center justify-center p-4 text-center sm:p-0">
                    <div
                        class="relative transform overflow-hidden rounded-lg bg-white text-left shadow-xl transition-all sm:my-8 sm:w-full sm:max-w-4xl">
                        <div class="bg-white px-4 pt-5 pb-4 sm:p-6 sm:pb-4">
                            <div class="sm:flex sm:items-start">
                                <div class="mt-3 text-center sm:mt-0 sm:text-left w-full">
                                    <h3
                                        class="text-lg leading-6 font-medium text-gray-900 pb-2 border-b mb-4 flex justify-between items-center">
                                        <span id="modalCustomerName">Customer Name</span>
                                        <span class="text-sm text-gray-500 font-normal" id="modalOrderDate">Date</span>
                                    </h3>

                                    <!-- Desktop View: Table -->
                                    <div class="hidden sm:block overflow-x-auto">
                                        <table class="min-w-full divide-y divide-gray-200">
                                            <thead class="bg-gray-50">
                                                <tr>
                                                    <th
                                                        class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                        รหัสสินค้า</th>
                                                    <th
                                                        class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                        รายการสินค้า</th>
                                                    <th
                                                        class="px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                        ราคา/หน่วย</th>
                                                    <th
                                                        class="px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                        จำนวน</th>
                                                    <th
                                                        class="px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider">
                                                        รวม</th>
                                                </tr>
                                            </thead>
                                            <tbody class="bg-white divide-y divide-gray-200" id="modalOrderItemsBody">
                                                <!-- Desktop Items will be injected here -->
                                            </tbody>
                                        </table>
                                    </div>

                                    <!-- Mobile View: Cards -->
                                    <div class="sm:hidden space-y-4" id="modalOrderItemsList">
                                        <!-- Mobile Items will be injected here -->
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="bg-gray-50 px-4 py-3 sm:px-6 sm:flex sm:flex-row-reverse">
                            <button type="button" id="closeModalBtn"
                                class="w-full inline-flex justify-center rounded-md border border-gray-300 shadow-sm px-4 py-2 bg-white text-base font-medium text-gray-700 hover:bg-gray-50 focus:outline-none sm:ml-3 sm:w-auto sm:text-sm">
                                ปิด
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <script id="orders-data" type="application/json">
        <%- JSON.stringify(orders) %>
    </script>

        <!-- Pagination -->
        <% if (totalPages> 1) { %>
            <div
                class="bg-white px-4 py-3 flex items-center justify-between border-t border-gray-200 sm:px-6 mt-4 rounded-lg shadow">
                <!-- Mobile Pagination -->
                <div class="flex-1 flex justify-between sm:hidden">
                    <% if (currentPage> 1) { %>
                        <a href="?page=<%= Math.max(1, currentPage - 1) %>&q=<%= query %>"
                            class="relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50">
                            ก่อนหน้า
                        </a>
                        <% } else { %>
                            <div></div>
                            <% } %>

                                <% if (currentPage < totalPages) { %>
                                    <a href="?page=<%= Math.min(totalPages, currentPage + 1) %>&q=<%= query %>"
                                        class="ml-3 relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50">
                                        ถัดไป
                                    </a>
                                    <% } %>
                </div>

                <!-- Desktop Pagination -->
                <div class="hidden sm:flex-1 sm:flex sm:items-center sm:justify-between">
                    <div>
                        <p class="text-sm text-gray-700">
                            หน้า <span class="font-medium">
                                <%= currentPage %>
                            </span> จาก <span class="font-medium">
                                <%= totalPages %>
                            </span>
                        </p>
                    </div>
                    <div>
                        <nav class="relative z-0 inline-flex rounded-md shadow-sm -space-x-px" aria-label="Pagination">
                            <!-- Previous Page -->
                            <% if (currentPage> 1) { %>
                                <a href="?page=<%= currentPage - 1 %>&q=<%= query %>"
                                    class="relative inline-flex items-center px-2 py-2 rounded-l-md border border-gray-300 bg-white text-sm font-medium text-gray-500 hover:bg-gray-50">
                                    <span class="sr-only">ก่อนหน้า</span>
                                    <svg class="h-5 w-5" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20"
                                        fill="currentColor" aria-hidden="true">
                                        <path fill-rule="evenodd"
                                            d="M12.707 5.293a1 1 0 010 1.414L9.414 10l3.293 3.293a1 1 0 01-1.414 1.414l-4-4a1 1 0 010-1.414l4-4a1 1 0 011.414 0z"
                                            clip-rule="evenodd" />
                                    </svg>
                                </a>
                                <% } %>

                                    <!-- Page Numbers -->
                                    <% let startPage=Math.max(1, currentPage - 2); let endPage=Math.min(totalPages,
                                        currentPage + 2); if (currentPage <=3) { endPage=Math.min(5, totalPages); } if
                                        (currentPage>= totalPages - 2) {
                                        startPage = Math.max(1, totalPages - 4);
                                        }

                                        if (startPage > 1) {
                                        %>
                                        <a href="?page=1&q=<%= query %>"
                                            class="bg-white border-gray-300 text-gray-500 hover:bg-gray-50 relative inline-flex items-center px-4 py-2 border text-sm font-medium">1</a>
                                        <% if (startPage> 2) { %>
                                            <span
                                                class="relative inline-flex items-center px-4 py-2 border border-gray-300 bg-white text-sm font-medium text-gray-700">...</span>
                                            <% } %>
                                                <% } %>

                                                    <% for(let i=startPage; i <=endPage; i++) { %>
                                                        <% if (i===currentPage) { %>
                                                            <span aria-current="page"
                                                                class="z-10 bg-blue-50 border-blue-500 text-blue-600 relative inline-flex items-center px-4 py-2 border text-sm font-medium">
                                                                <%= i %>
                                                            </span>
                                                            <% } else { %>
                                                                <a href="?page=<%= i %>&q=<%= query %>"
                                                                    class="bg-white border-gray-300 text-gray-500 hover:bg-gray-50 relative inline-flex items-center px-4 py-2 border text-sm font-medium">
                                                                    <%= i %>
                                                                </a>
                                                                <% } %>
                                                                    <% } %>

                                                                        <% if (endPage < totalPages) { %>
                                                                            <% if (endPage < totalPages - 1) { %>
                                                                                <span
                                                                                    class="relative inline-flex items-center px-4 py-2 border border-gray-300 bg-white text-sm font-medium text-gray-700">...</span>
                                                                                <% } %>
                                                                                    <a href="?page=<%= totalPages %>&q=<%= query %>"
                                                                                        class="bg-white border-gray-300 text-gray-500 hover:bg-gray-50 relative inline-flex items-center px-4 py-2 border text-sm font-medium">
                                                                                        <%= totalPages %>
                                                                                    </a>
                                                                                    <% } %>

